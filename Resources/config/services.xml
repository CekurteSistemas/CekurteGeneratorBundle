<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="cekurte_generator.twig.extension.roleextension" class="Cekurte\GeneratorBundle\Twig\Extension\RoleExtension">
            <tag name="twig.extension" />
            <argument type="service" id="service_container" />
        </service>
        <service id="cekurte_generator.twig.extension.actionextension" class="Cekurte\GeneratorBundle\Twig\Extension\ActionExtension">
            <tag name="twig.extension" />
            <argument type="service" id="service_container" />
        </service>
        <service id="cekurte_generator.twig.extension.paginationextension" class="Cekurte\GeneratorBundle\Twig\Extension\PaginationExtension">
            <tag name="twig.extension" />
            <argument type="service" id="request_stack" />
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
    </services>
</container>
